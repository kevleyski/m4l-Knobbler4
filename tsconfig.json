{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "CommonJS",
    "target": "es5",
    "strict": false,
    "noImplicitAny": true,
    "allowUnreachableCode": true,
    "sourceMap": false,
    "moduleDetection": "force",
    "lib": ["es6"],
    "outDir": "Project"
  },
  "include": ["src/*.ts", "types/*.d.ts"]
}
